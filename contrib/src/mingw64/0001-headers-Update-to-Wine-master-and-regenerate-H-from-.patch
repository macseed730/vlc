+        __C89_NAMELESS struct {
+            UINT Type : 1;
+            UINT RGB_Range : 1;
+            UINT YCbCr_Matrix : 1;
+            UINT YCbCr_xvYCC : 1;
+        } __C89_NAMELESSSTRUCTNAME;
+        UINT Value;
+    } __C89_NAMELESSUNIONNAME;
 } DXVAHD_STREAM_STATE_INPUT_COLOR_SPACE_DATA;
 typedef struct _DXVAHD_STREAM_STATE_LUMA_KEY_DATA {
