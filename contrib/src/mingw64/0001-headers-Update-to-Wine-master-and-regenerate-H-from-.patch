 typedef struct _DXVAHD_BLT_STATE_PRIVATE_DATA {
     GUID Guid;
@@ -332,10 +338,15 @@ typedef struct _DXVAHD_STREAM_STATE_FRAME_FORMAT_DATA {
     DXVAHD_FRAME_FORMAT FrameFormat;
